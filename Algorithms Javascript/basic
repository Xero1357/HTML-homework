// // //

var A = [[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]; 
let maxValues = [];

for (let i = 0; i < A.length; i++) {
    let max = A[i][0]; 
    
    for (let j = 1; j < A[i].length; j++) {
        if (A[i][j] > max) {
            max = A[i][j]; 
        }
    }
    
    maxValues.push(max); 
}

console.log(maxValues);

//  // //
var A = 100;

while (A > 1) {
    let B; 

    if (A % 3 === 0) {
        B = A / 3;
    } else if ((A + 1) % 3 === 0) {
        B = (A + 1) / 3;
    } else if ((A - 1) % 3 === 0) {
        B = (A - 1) / 3;
    }

    A = B; 
    console.log(B)
}
// // 

var num = prompt("What number do you want to find the factorial of?");

num = Number(num);

var factorial = function(n) {
    if (n < 0) {
        return "Factorial is not defined for negative numbers.";
    } else if (n === 0) {
        return 1;
    } else {
        return n * factorial(n - 1);
    }
};

console.log(factorial(num));

