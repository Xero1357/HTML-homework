// 5
var x = [2, 12, -3, 0];
let max = x[0]; 

for (let i = 1; i < x.length; i++) {
    if (x[i] > max) {
        max = x[i]; 
    }
}
console.log(max); 
// 6

var A = [2, 12, -3, 0];
let total = 0;

for (let i = 0; i < A.length; i++) {
  total += A[i];
}

var B = total / A.length;

console.log(B);  
