// tuesday

var bunnies = 4;

function BunnyEars(bunnies) {
  var totalEars = 0; 
  
  for (var i = 0; i < bunnies; i++) {
    totalEars += 2; 
  }
  
  return totalEars; 
}

console.log(BunnyEars(bunnies)); 

// wednessday

var x = [3, 2]

function PowerN(base,exponent){
  let result = 1
  for(let i=0 ; i < exponent ; i++){
    result *= base

  }
  return result;
}
console.log(PowerN(x[0],x[1]))
